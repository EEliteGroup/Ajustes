su -c "echo "1200000" > /sys/devices/system/cpu/cpu0/cpufreq/scaling_min_freq"
su -c "echo "1200000" > /sys/devices/system/cpu/cpu0/cpufreq/scaling_max_freq"
su -c "echo "1200000" > /sys/devices/system/cpu/cpu1/cpufreq/scaling_min_freq"
su -c "echo "1200000" > /sys/devices/system/cpu/cpu1/cpufreq/scaling_max_freq"
su -c "echo "1200000" > /sys/devices/system/cpu/cpu2/cpufreq/scaling_min_freq"
su -c "echo "1200000" > /sys/devices/system/cpu/cpu2/cpufreq/scaling_max_freq"
su -c "echo "1200000" > /sys/devices/system/cpu/cpu3/cpufreq/scaling_min_freq"
su -c "echo "1200000" > /sys/devices/system/cpu/cpu3/cpufreq/scaling_max_freq"
rm -rf /data/local/cpumx
